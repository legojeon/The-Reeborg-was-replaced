{
  "robots": [
    {
      "x": 1,
      "y": 1,
      "_prev_x": 1,
      "_prev_y": 1,
      "_prev_orientation": 0,
      "objects": {
        "token": 100
      },
      "_orientation": 0,
      "_is_leaky": true,
      "_trace_history": [],
      "_trace_style": "default",
      "_trace_color": "seagreen",
      "__id": 5,
      "model": "classic"
    }
  ],
  "walls": {},
  "description": [
    "<맵: 아토331>"
  ],
  "small_tiles": false,
  "rows": 10,
  "cols": 10,
  "onload": [
    "var choice, worldSize;",
    "choice = RUR.randint(1, 8);",
    "if (choice == 1) {",
    "    worldSize = 6;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "if (choice == 2) {",
    "    worldSize = 8;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "if (choice == 3) {",
    "    worldSize = 10;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "if (choice == 4) {",
    "    worldSize = 12;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "if (choice == 5) {",
    "    worldSize = 14;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "if (choice == 6) {",
    "    worldSize = 16;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "if (choice == 7) {",
    "    worldSize = 18;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "if (choice == 8) {",
    "    worldSize = 20;",
    "    RUR.set_world_size(worldSize, worldSize);",
    "}",
    "var n, side, leng;",
    "for (n=1; n<=worldSize/2; n++) {",
    "    for (side=1; side<=4; side++) {",
    "        // south side",
    "        if (side == 1) {",
    "            for (leng=n; leng<=worldSize-n; leng++) {",
    "                RUR.add_wall('north', leng, n);",
    "            }",
    "        }",
    "        // east side",
    "        if (side == 2) {",
    "            for (leng=n+1; leng<=worldSize-n; leng++) {",
    "                RUR.add_wall('west', worldSize+1-n, leng);",
    "            }",
    "        }",
    "        // north side",
    "        if (side == 3) {",
    "            for (leng=n+1; leng<=worldSize-n; leng++) {",
    "                RUR.add_wall('south', leng, worldSize+1-n);",
    "            }",
    "        }",
    "        // west side",
    "        if (side == 4) {",
    "            for (leng=n+2; leng<=worldSize-n; leng++) {",
    "                RUR.add_wall('east', n, leng);",
    "            }",
    "        }",
    "    }",
    "}",
    "RUR.add_object('token', worldSize/2, worldSize/2+1, {'number': 1});",
    "var xpos, ypos;",
    "for (xpos=1; xpos<=worldSize; xpos++) {",
    "    if (xpos != worldSize/2) {",
    "        RUR.add_object('token', xpos, worldSize/2+1, {'number': RUR.randint(1, 9)});",
    "    }",
    "}",
    "for (ypos=1; ypos<=worldSize; ypos++) {",
    "    if (ypos != worldSize/2+1) {",
    "        RUR.add_object('token', worldSize/2, ypos, {'number': RUR.randint(1, 9)});",
    "    }",
    "}",
    "RUR.add_object('token', 1, worldSize, {'number': RUR.randint(1, 9)});",
    "RUR.add_object('token', worldSize, 1, {'number': RUR.randint(1, 9)});",
    "RUR.add_object('token', worldSize, worldSize, {'number': RUR.randint(1, 9)});"
  ]
}