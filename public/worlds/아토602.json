    "onload": [
        "var matrix = []",
        "",
        "function shuffle(array) {",
        "  return array.sort(() => Math.random() - 0.5);",
        "}",
        "",
        "function randint(min, max) {",
        "  return Math.floor(Math.random() * (max - min + 1)) + min;",
        "};",
        "",
        "let c1 = randint(1, 15);",
        "for (var i = 0; i < c1; i++) {",
        "    matrix.push(1);",
        "}",
        "let c2 = randint(1, 15);",
        "for (var i = 0; i < c2; i++) {",
        "    matrix.push(2);",
        "}",
        "let c3 = 36 - (c1+c2);",
        "for (var i = 0; i < c3; i++) {",
        "    matrix.push(3);",
        "}",
        "matrix = shuffle(matrix);",
        "",
        "// 1이 있는 위치에만 당근을 추가하기",
        "matrix.forEach( function(item, index) {",
        "        var x = index % 6 +3;",
        "        var y = parseInt(index / 6 + 3);",
        "        RUR.add_object('carrot', x, y, {'number': item});",
        "});",
        "RUR.add_object('carrot', 1, 10, {'goal': true, 'number': c1});",
        "RUR.add_object('carrot', 2, 10, {'goal': true, 'number': c2*2});",
        "RUR.add_object('carrot', 3, 10, {'goal': true, 'number': c3*3});"
    ]
