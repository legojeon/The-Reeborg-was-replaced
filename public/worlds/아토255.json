        "function shuffle(array) {",
        "    return array.sort(() => Math.random() - 0.5);",
        "}",
        "",
        "function randint(min, max) {",
        "    return Math.floor(Math.random() * (max - min + 1)) + min;",
        "};",
        "",
        "let matrix = [];",
        "let total = 36;",
        "let cnt = randint(5, 10);",
        "// 0 채우기",
        "for(let x=0; x<cnt; x++) {",
        "    matrix.push(0);",
        "}",
        "// 1 채우기",
        "cnt = randint(10, 20);",
        "for(let x=0; x<10; x++) {",
        "    matrix.push(1);",
        "}",
        "",
        "cnt = total - matrix.length;",
        "for(let x=0; x<cnt; x++) {",
        "    matrix.push(2)",
        "}",
        "",
        "matrix = shuffle(matrix);",
        "",
        "matrix.forEach( function(carrot, idx) {",
        "    let x = idx % 6 + 3;",
        "    let y = parseInt(idx / 6 + 3);",
        "    RUR.add_object('carrot', x, y, {'number': carrot});",
